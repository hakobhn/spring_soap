	  <xsd:simpleType name="Decimal5_2">
		    <xsd:annotation>
			      <xsd:documentation>Defines decimal numbers with up to two places. Allowed values are for example:
0.00
0.0
0
5.10
5.1
5
5.00
999.99</xsd:documentation>
		    </xsd:annotation>
		    <xsd:restriction base="xsd:decimal">
			      <xsd:pattern value="(0|[1-9][0-9]*)(\.[0-9]{1,2})?"/>
			      <xsd:maxInclusive value="999.99"/>
			      <xsd:minInclusive value="0.00"/>
		    </xsd:restriction>
	  </xsd:simpleType>
